#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	/*
	
	int x;
	cin>>x;
	if(x==1){
	    cout<<"Monday";
	}
	else if(x==2){
	    cout<<"Tuesday";
	}
	else if(x==3){
	    cout<<"Wednesday";
	}
	else if(x==4){
	    cout<<"Thursday";
	}
	else if(x==5){
	    cout<<"Friday";
	}
	else if(x==6){
	    cout<<"Saturday";
	}
	else{
	    cout<<"Sunday";
	}
	*/
	int day;
	cin >> day;
	switch(day){
	    case 1:
	        cout<<"Monday";
	        break;
	    case 2:
	        cout<<"Tuesday";
	        break;
	    case 3:
	        cout<<"Wednesday";
	        break;
	    case 4:
	        cout<<"Thursday";
	        break;
	    case 5:
	        cout<<"Friday";
	        break;
	    case 6:
	        cout<<"Saturday";
	        break;
	    case 7:
	        cout<<"Sunday";
	    default:
	        cout<<"Inavalid check"<<endl;
	}
	cout<<"invalid";
	return 0;
}

